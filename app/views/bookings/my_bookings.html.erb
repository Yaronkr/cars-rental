<div class="container">
  <h1 style="color: #293462">My bookings</h1>
  <ul class="card">
    <% @bookings.each do |booking| %>
      <li> *********************************** </li>
      <li>Booked car:<%= " #{booking.car.make} #{booking.car.model}" %> </li>
      <li>Start Date: <%= booking.start_date %> </li>
      <li>End Date: <%= booking.end_date %> </li>
      <li>Total Price: <%= booking.total_price %> </li>
      <li> *********************************** </li>
    <% end %>
  </ul>
</div>
