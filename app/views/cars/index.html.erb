<h1>Browse our cars</h1>
<p>You are only one step away to rent a car</p>

<ul>
  <% @cars.each do |car| %>
    <li><%= link_to "#{car.make}, #{car.model} - $#{car.price_day}", car_path(car) %> |  <%= link_to 'Edit', edit_car_path(car) %> | owned by: <%= car.user.email %> |
    </li>
  <% end %>
</ul>
